#include<bits/stdc++.h>
using namespace std;
void printpairs(int arr[],int n)
{
    sort(arr,arr+n);
   for(int i=0;i<n;i++)
   {
    
       for(int j=i+1;j<n;j++)
       {
           cout<<"["<<arr[i]<<", "<<arr[j]<<"] ";
       }
    
   }
}
int main()
{
     int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
       cin>>arr[i];
    }
    printpairs(arr, n);
    return 0;
}
