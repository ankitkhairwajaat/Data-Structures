#include <bits/stdc++.h>
using namespace std;
int linearsearch(int arr[], int n, int key)
{
    for (int i = 0; i < n; i++)
    {
        if (arr[i] == key)
        {
            return i;
        }
    }
    return -1;
}
int main()
{

    int n;
    int arr[n];
    int key;
    cout << " Enter the Key ";

    cin >> key;
    cout << " Enter the elements ";
    cin >> n;
    for (int i = 0; i < n; i++)
    {
        cin >> arr[i];
    }
    cout << linearsearch(arr, n, key);
    return 0;
}
