#include<bits/stdc++.h>
using namespace std;
int main()
{
     char ch;
     int alpha=0;
     int digit=0;
     int space=0;
     ch=cin.get();
     while(ch!='\n')
     {
         if(ch<='1'&& ch<='9')
         {
             digit++;
         }
         else if((ch>='a'&& ch<='z')||(ch>='A'&&ch<='Z'))
         {
             alpha++;
         }
         else
         {
             space++;
         }
         ch=cin.get();
     }
     cout<<digit<<" "<<alpha<<" "<<space<<" "<<endl;
    return 0;
}
