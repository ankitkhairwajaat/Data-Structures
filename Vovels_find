#include <bits/stdc++.h>
using namespace std;
string Vovel(string S)
{
    int n = S.size();
    string O;
    for(auto x:S)
    {
        if(x=='a'||x=='e'||x=='i'||x=='o'||x=='u')
        {
            O.push_back(x);
        }

    }
    return O;
}
int main()
{
    string S;
    getline(cin, S);
    cout << Vovel(S);
    return 0;
}
