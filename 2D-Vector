#include <bits/stdc++.h>
using namespace std;
int main()
{
    vector<vector <int>> arr = {{1, 2, 3},
                            {2, 3, 4},
                            {4, 5, 6}};

        arr[0][0]=33;

    for (int l = 0; l < arr.size(); l++)
    {
        for (auto x:arr[l])
        {
            cout << x <<" ";
        }
        cout<<endl;
    }
    cout << arr.size() << endl;
    cout << arr.capacity();

    return 0;
}
